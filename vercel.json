{
  "version": 2,
  "framework": "vite",
  "installCommand": "cd web && npm ci || npm install",
  "buildCommand": "cd web && npm run build",
  "devCommand": "cd web && npm run dev",
  "outputDirectory": "web/dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/" }
  ]
}
