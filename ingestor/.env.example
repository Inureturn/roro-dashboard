# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE=your-service-role-key-here

# AISStream Configuration
AISSTREAM_KEY=your-aisstream-api-key-here

# Fleet Configuration
# My Fleet MMSIs (vessels you own/operate) - marked as is_my_fleet=true
FLEET_MMSIS=357170000,352808000,352001129,355297000,356005000

# Competitor MMSIs (vessels to track but mark as competitors) - marked as is_competitor=true
COMPETITOR_MMSIS=249901000,229077000,229076000,219927000,352001162,355137000,352001920,373817000,249904000

# Bounding Boxes
# Semicolon-separated list of [[lon_min,lat_min],[lon_max,lat_max]] pairs
# Example: Korea Strait + Mediterranean Sea
BBOX_JSON=[[124,33],[132,39]];[[-6,30],[36,46]]

# Optional: Logging
# Set to 'debug' for verbose output, 'info' for standard output
LOG_LEVEL=info
